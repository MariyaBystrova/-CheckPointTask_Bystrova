<?xml version="1.0" encoding="UTF-8"?>

<Configuration>
	<Appenders>
		<file append="false" filename="logs/CyclingGoods.log" name="log">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n" />
		</file> 
		<!-- <Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n" />
		</Console> -->
	</Appenders>

	<Loggers>

		<Root level="info">
			<AppenderRef ref="log" />
		</Root>
	</Loggers>

</Configuration>